{
  "name": "keystone-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run watch",
    "serve": "nodemon dist/keystone.js",
    "test": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "build-sass": "node-sass src/public/styles/site.scss -o dist/public/styles/",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-sass": "node-sass -w src/public/styles/site.scss -o dist/public/styles/",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/keystone.js"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bootstrap": "^4.2.1",
    "dotenv": "^6.2.0",
    "keystone": "^4.0.0",
    "keystone-email": "^1.1.0",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.119",
    "@types/mongoose": "^4.7.9",
    "@types/node": "^10.12.18",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.18.9",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.5",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}
